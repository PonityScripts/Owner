
local key = _G.Key
local check = "https://heazerwhitelist001.000webhostapp.com/check.php?key=" .. key
if game:HttpGet(check) == "Whitelisted" then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Icon = "rbxassetid://6034509993";
            Title = "BoozyX Hub Notify", 
            Text = "✅ Whitelisted"
        }) 
loadstring(game:HttpGet("https://heazerwhitelist001.000webhostapp.com/script.lua"))()
else
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Icon = "rbxassetid://6034509993";
            Title = "BoozyX Hub Notify", 
            Text = "❌  Key Not Found"
        })
        wait(10)
game.Players.LocalPlayer:Kick("Wrong Key! Please Rejoin And Try Again.")
wait(5)
game:Shutdown()
end
